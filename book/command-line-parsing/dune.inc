(rule
 (target findlib.conf)
 (action (write-file %{target} "")))

(alias
 (name   runtest)
 (deps   (:x README.md)
         (:conf findlib.conf)
         (package mdx)
         (package core)
         (package ppx_jane)
         (:y15 ../../examples/code/command-line-parsing/cal_add_days/cal.ml)
         (:y14 ../../examples/code/command-line-parsing/cal_add_interactive/cal.ml)
         (:y13 ../../examples/code/command-line-parsing/cal_add_interactive2/cal.ml)
         (:y12 ../../examples/code/command-line-parsing/cal_add_sub_days/cal.ml)
         (:y11 ../../examples/code/command-line-parsing/md5/md5.ml)
         (:y10 ../../examples/code/command-line-parsing/md5_as_filename/md5.ml)
         (:y9 ../../examples/code/command-line-parsing/md5_let_syntax/md5.ml)
         (:y8 ../../examples/code/command-line-parsing/md5_let_syntax2/md5.ml)
         (:y7 ../../examples/code/command-line-parsing/md5_multiarg/md5.ml)
         (:y6 ../../examples/code/command-line-parsing/md5_sequence/md5.ml)
         (:y5 ../../examples/code/command-line-parsing/md5_succinct/md5.ml)
         (:y4 ../../examples/code/command-line-parsing/md5_with_custom_arg/md5.ml)
         (:y3 ../../examples/code/command-line-parsing/md5_with_default_file/md5.ml)
         (:y2 ../../examples/code/command-line-parsing/md5_with_flags/md5.ml)
         (:y1 ../../examples/code/command-line-parsing/md5_with_optional_file/md5.ml)
         (:y0 ../../examples/code/command-line-parsing/md5_with_optional_file_broken/md5.ml)
         (source_tree ../../examples/code/command-line-parsing/cal_add_days)
         (source_tree ../../examples/code/command-line-parsing/cal_add_interactive2)
         (source_tree ../../examples/code/command-line-parsing/cal_add_sub_days)
         (source_tree ../../examples/code/command-line-parsing/md5)
         (source_tree ../../examples/code/command-line-parsing/md5_multiarg)
         (source_tree ../../examples/code/command-line-parsing/md5_sequence)
         (source_tree ../../examples/code/command-line-parsing/md5_with_custom_arg)
         (source_tree ../../examples/code/command-line-parsing/md5_with_default_file)
         (source_tree ../../examples/code/command-line-parsing/md5_with_flags)
         (source_tree ../../examples/code/command-line-parsing/md5_with_optional_file)
         (source_tree ../../examples/code/command-line-parsing/md5_with_optional_file_broken)
         prelude.ml)
 (locks ../../global-lock)
 (action (progn
           (setenv OCAMLFIND_CONF %{conf} (run ocaml-mdx test --prelude=prelude.ml --direction=to-ml %{x}))
           (diff? %{y15} %{y15}.corrected)
           (diff? %{y14} %{y14}.corrected)
           (diff? %{y13} %{y13}.corrected)
           (diff? %{y12} %{y12}.corrected)
           (diff? %{y11} %{y11}.corrected)
           (diff? %{y10} %{y10}.corrected)
           (diff? %{y9} %{y9}.corrected)
           (diff? %{y8} %{y8}.corrected)
           (diff? %{y7} %{y7}.corrected)
           (diff? %{y6} %{y6}.corrected)
           (diff? %{y5} %{y5}.corrected)
           (diff? %{y4} %{y4}.corrected)
           (diff? %{y3} %{y3}.corrected)
           (diff? %{y2} %{y2}.corrected)
           (diff? %{y1} %{y1}.corrected)
           (diff? %{y0} %{y0}.corrected)
           (diff? %{x} %{x}.corrected))))
