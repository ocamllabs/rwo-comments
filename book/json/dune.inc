(alias
 (name runtest)
 (deps
  (:x README.md)
  (package core)
  (package mdx)
  (package ppx_jane)
  (package yojson)
  (:y7 examples/json/github_j_excerpt.mli)
  (:y6 examples/json/github_org_info/github_org_info.ml)
  (:y5 examples/json/list_excerpt.mli)
  (:y4 examples/json/parse_book/parse_book.ml)
  (:y3 examples/json/read_json/read_json.ml)
  (:y2 examples/json/yojson_basic.mli)
  (:y1 examples/json/yojson_basic_simple.mli)
  (:y0 examples/json/yojson_safe.mli)
  (source_tree examples/json)
  prelude.ml)
 (locks ../../global-lock)
 (action
  (progn
   (run ocaml-mdx test --prelude=prelude.ml --direction=to-md
     --root=examples/json %{x})
   (diff? %{x} %{x}.corrected)
   (diff? %{y7} %{y7}.corrected)
   (diff? %{y6} %{y6}.corrected)
   (diff? %{y5} %{y5}.corrected)
   (diff? %{y4} %{y4}.corrected)
   (diff? %{y3} %{y3}.corrected)
   (diff? %{y2} %{y2}.corrected)
   (diff? %{y1} %{y1}.corrected)
   (diff? %{y0} %{y0}.corrected))))

(alias
 (name runtest-all)
 (deps
  (:x README.md)
