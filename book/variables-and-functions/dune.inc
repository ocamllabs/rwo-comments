(alias
 (name runtest)
 (deps
  (:x README.md)
  (package core)
  (package mdx)
  (package ppx_jane)
  (:y5 examples/abs_diff.mli)
  (:y4 examples/htable_sig1.ml)
  (:y3 examples/htable_sig2.ml)
  (:y2 examples/numerical_deriv_alt_sig.mli)
  (:y1 examples/substring_sig1.ml)
  (:y0 examples/substring_sig2.ml)
  prelude.ml)
 (locks ../../global-lock)
 (action
  (progn
   (run ocaml-mdx test --prelude=prelude.ml --direction=to-md %{x})
   (diff? %{x} %{x}.corrected)
   (diff? %{y5} %{y5}.corrected)
   (diff? %{y4} %{y4}.corrected)
   (diff? %{y3} %{y3}.corrected)
   (diff? %{y2} %{y2}.corrected)
   (diff? %{y1} %{y1}.corrected)
   (diff? %{y0} %{y0}.corrected))))
